<div id="<%= dom_id profile %>">
  <p>
    <strong>Age:</strong>
    <%= profile.age %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= profile.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= profile.last_name %>
  </p>

  <p>
    <strong>Interests:</strong>
    <%= profile.interests %>
  </p>

  <p>
    <strong>About:</strong>
    <%= profile.about %>
  </p>

  <p>
    <strong>User:</strong>
    <%= profile.user_id %>
  </p>

</div>
